{
  "dashboard": {
    "id": null,
    "title": "AET Resource Usage",
    "tags": ["aet", "resources", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(aet_cpu_usage_percent, instance)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(aet_cpu_usage_percent{instance=~\"$instance\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(aet_memory_usage_bytes{instance=~\"$instance\"}) / 1024 / 1024 / 1024",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "gbytes",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 4
                },
                {
                  "color": "red",
                  "value": 8
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Disk Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(aet_disk_usage_bytes{instance=~\"$instance\"}) / 1024 / 1024 / 1024",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "gbytes",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Event Log Size",
        "type": "stat",
        "targets": [
          {
            "expr": "aet_event_log_size_bytes{instance=~\"$instance\"} / 1024 / 1024",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "mbytes",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "CPU Usage Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_cpu_usage_percent{instance=~\"$instance\"}",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Memory Usage Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_memory_usage_bytes{instance=~\"$instance\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Disk Usage by Path",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_disk_usage_bytes{instance=~\"$instance\"} / 1024 / 1024",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Disk Usage (MB)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Knowledge Manager Database Growth",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_km_database_size_bytes{instance=~\"$instance\"} / 1024 / 1024",
            "legendFormat": "KM Database (MB)",
            "refId": "A"
          },
          {
            "expr": "aet_event_log_size_bytes{instance=~\"$instance\"} / 1024 / 1024",
            "legendFormat": "Event Log (MB)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Size (MB)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "File Registry Statistics",
        "type": "table",
        "targets": [
          {
            "expr": "aet_registered_files_total{instance=~\"$instance\"}",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "aet_km_file_summaries_total{instance=~\"$instance\"}",
            "format": "table",
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "Value #A": "Registered Files",
                "Value #B": "File Summaries"
              }
            }
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "Resource Usage Efficiency",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_tasks_total{instance=~\"$instance\"}[5m]) / (aet_cpu_usage_percent{instance=~\"$instance\"} / 100)",
            "legendFormat": "Tasks per CPU %",
            "refId": "A"
          },
          {
            "expr": "rate(aet_tasks_total{instance=~\"$instance\"}[5m]) / (aet_memory_usage_bytes{instance=~\"$instance\"} / 1024 / 1024 / 1024)",
            "legendFormat": "Tasks per GB Memory",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Efficiency Ratio",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 11,
        "title": "Resource Alerts Status",
        "type": "stat",
        "targets": [
          {
            "expr": "count(aet_cpu_usage_percent{instance=~\"$instance\"} > 80)",
            "legendFormat": "High CPU",
            "refId": "A"
          },
          {
            "expr": "count(aet_memory_usage_bytes{instance=~\"$instance\"} / 1024 / 1024 / 1024 > 6)",
            "legendFormat": "High Memory",
            "refId": "B"
          },
          {
            "expr": "count(aet_disk_usage_bytes{instance=~\"$instance\"} / 1024 / 1024 / 1024 > 80)",
            "legendFormat": "High Disk",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 12,
        "title": "System Load vs Task Throughput",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_cpu_usage_percent{instance=~\"$instance\"}",
            "legendFormat": "CPU Usage (%)",
            "refId": "A"
          },
          {
            "expr": "rate(aet_tasks_total{instance=~\"$instance\"}[5m]) * 100",
            "legendFormat": "Task Rate (scaled x100)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage / Scaled Rate",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 13,
        "title": "Memory Growth Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_memory_usage_bytes{instance=~\"$instance\"}[5m]) / 1024 / 1024",
            "legendFormat": "Memory Growth (MB/sec)",
            "refId": "A"
          },
          {
            "expr": "rate(aet_km_database_size_bytes{instance=~\"$instance\"}[5m]) / 1024",
            "legendFormat": "DB Growth (KB/sec)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Growth Rate",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 14,
        "title": "Observability Overhead",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_metrics_overhead_ms{instance=~\"$instance\"}",
            "legendFormat": "Metrics Overhead (ms)",
            "refId": "A"
          },
          {
            "expr": "aet_tracing_overhead_ms{instance=~\"$instance\"}",
            "legendFormat": "Tracing Overhead (ms)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Overhead (ms)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 40
        }
      }
    ]
  }
}