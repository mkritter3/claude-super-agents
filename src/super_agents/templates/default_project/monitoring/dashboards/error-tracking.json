{
  "dashboard": {
    "id": null,
    "title": "AET Error Tracking",
    "tags": ["aet", "errors", "debugging"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(aet_agent_errors_total, instance)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "agent",
          "type": "query",
          "query": "label_values(aet_agent_errors_total, agent)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Overall Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(aet_agent_errors_total{instance=~\"$instance\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Task Failure Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(aet_tasks_total{status=\"failure\", instance=~\"$instance\"}[5m]) / rate(aet_tasks_total{instance=~\"$instance\"}[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 15
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Active Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\", instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Error Trend",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(aet_agent_errors_total{instance=~\"$instance\"}[5m]) - rate(aet_agent_errors_total{instance=~\"$instance\"}[10m] offset 5m)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Error Rate by Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_agent_errors_total{agent=~\"$agent\", instance=~\"$instance\"}[5m])",
            "legendFormat": "{{agent}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Task Failure Rate by Agent",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_tasks_total{status=\"failure\", agent=~\"$agent\", instance=~\"$instance\"}[5m])",
            "legendFormat": "{{agent}} - {{mode}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Failures/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Error Types Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (error_type) (rate(aet_agent_errors_total{instance=~\"$instance\"}[5m]))",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "Error Distribution by Agent",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (agent) (rate(aet_agent_errors_total{agent=~\"$agent\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "{{agent}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "Hourly Error Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(aet_agent_errors_total{instance=~\"$instance\"}[1h]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 12
        }
      },
      {
        "id": 10,
        "title": "Recent Error Details",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(aet_agent_errors_total{instance=~\"$instance\"}[5m]))",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "Value": "Error Rate (/sec)"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 11,
        "title": "Error Rate vs Success Rate Correlation",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_agent_errors_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          },
          {
            "expr": "rate(aet_tasks_total{status=\"success\", instance=~\"$instance\"}[5m])",
            "legendFormat": "Success Rate",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Rate (/sec)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 12,
        "title": "Error Rate by Time of Day",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (hour) (rate(aet_agent_errors_total{instance=~\"$instance\"}[1h]))",
            "legendFormat": "Hour {{hour}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/hour",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 13,
        "title": "System Health Impact",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_system_health_score",
            "legendFormat": "Health Score",
            "refId": "A"
          },
          {
            "expr": "rate(aet_agent_errors_total{instance=~\"$instance\"}[5m]) * 100",
            "legendFormat": "Error Rate (scaled)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Score / Rate",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": 14,
        "title": "Error Recovery Time",
        "type": "graph",
        "targets": [
          {
            "expr": "time() - on() group_left() (aet_agent_errors_total{instance=~\"$instance\"} > 0)",
            "legendFormat": "Time since last error",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 36
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Error Spikes",
          "datasource": "prometheus",
          "enable": true,
          "expr": "increase(aet_agent_errors_total[5m]) > 5",
          "iconColor": "red",
          "textFormat": "Error spike detected"
        }
      ]
    }
  }
}