{
  "dashboard": {
    "id": null,
    "title": "AET System Overview",
    "tags": ["aet", "system", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(aet_tasks_total, instance)",
          "current": {
            "value": "All",
            "text": "All"
          },
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(aet_system_health_score)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Tasks",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(aet_active_tasks{instance=~\"$instance\"})",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Task Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(aet_tasks_total{status=\"success\", instance=~\"$instance\"}[5m]) / rate(aet_tasks_total{instance=~\"$instance\"}[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "time() - process_start_time_seconds{instance=~\"$instance\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Task Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_tasks_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{status}} - {{mode}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Tasks/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Task Duration Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(aet_task_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(aet_task_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(aet_task_duration_seconds_bucket{instance=~\"$instance\"}[5m]))",
            "legendFormat": "99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (seconds)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "System Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "aet_cpu_usage_percent{instance=~\"$instance\"}",
            "legendFormat": "CPU Usage (%)",
            "refId": "A"
          },
          {
            "expr": "aet_memory_usage_bytes{instance=~\"$instance\"} / 1024 / 1024 / 1024",
            "legendFormat": "Memory Usage (GB)",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage / GB",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_tasks_total{status=\"failure\", instance=~\"$instance\"}[5m])",
            "legendFormat": "Task Failures",
            "refId": "A"
          },
          {
            "expr": "rate(aet_agent_errors_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "Agent Errors - {{agent}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Knowledge Manager Status",
        "type": "table",
        "targets": [
          {
            "expr": "aet_registered_files_total{instance=~\"$instance\"}",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "aet_km_database_size_bytes{instance=~\"$instance\"} / 1024 / 1024",
            "format": "table",
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "Value #A": "Registered Files",
                "Value #B": "Database Size (MB)"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Event Log Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_events_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{event_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "File Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(aet_file_operations_total{instance=~\"$instance\"}[5m])",
            "legendFormat": "{{operation}} - {{status}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ]
  }
}